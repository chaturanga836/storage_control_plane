# Storage Control Plane Configuration
# Copy this file to .env and modify as needed

# Server Configuration
SERVER_ADDR=:8081

# Database Configuration  
SHARED_ROCKSDB_PATH=/data/shared_rocksdb
SHARED_CLICKHOUSE_DSN=tcp://localhost:9000?debug=true
POSTGRES_DSN=postgres://user:pass@localhost:5432/core

# Optional: Development settings
GO_ENV=development
LOG_LEVEL=info

# Optional: Security settings
# JWT_SECRET=your-secret-key-here
# TENANT_ENCRYPTION_KEY=your-encryption-key

# Optional: Storage settings
# WAL_FLUSH_THRESHOLD=1000
# PARQUET_BATCH_SIZE=10000
